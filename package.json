{
  "name": "@nonetss/none.api",
  "version": "1.0.2",
  "description": "Professional OpenAPI Explorer MCP server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nonetss/none.api.git"
  },
  "private": false,
  "type": "module",
  "bin": {
    "none-api": "build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "@modelcontextprotocol/sdk": "0.6.0",
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
