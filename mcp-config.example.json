{
  "mcpServers": {
    "openapi-explorer": {
      "command": "node",
      "args": [
        "/absolute/path/to/openapi/build/index.js"
      ],
      "env": {
        "OPENAPI_URL": "https://api.example.com/openapi.json",
        "AUTH_EMAIL": "user@example.com",
        "AUTH_PASSWORD": "password",
        "AUTH_PATH": "/api/v1/auth/login"
      }
    }
  }
}
